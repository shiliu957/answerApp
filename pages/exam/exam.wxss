/* pages/exam/exam.wxss */
.testPaper {
  margin: 0 auto;
  margin-top: 200rpx;
  background-image: url(data:image/png;base64,);
  background-size: 80rpx 80rpx;
  background-repeat: no-repeat;
  background-position: 5% 50%;
  display: flex;
  flex-direction: column;
  text-indent: 150rpx;
}

.testPaper text:nth-child(1) {
  font-weight: bold;
  margin-top: 50rpx;
}

.testPaper text:nth-child(2) {
  /* text-indent: 100rpx; */
  font-size: 16rpx;
  color: gray;
  margin-top: 10rpx;

}

.changeImg {
  background-image: url(data:image/png;base64,);
}

.btn {
  display: block;
  width: 600rpx;
  height: 120rpx;
  background-color: #409eff;
  border-radius: 30rpx;
  line-height: 120rpx;
  text-align: center;
  margin: 70rpx auto;
  color: #fff;
}
