.text {
  display: inline-block;
  font-weight: bolder;
  margin-top: 40rpx;
  margin-left: 30rpx;
}

.span {
  display: block;
  color: #adadad;
  font-size: 28rpx;
  font-weight: bold;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.color{
