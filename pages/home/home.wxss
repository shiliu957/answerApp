.main{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.swiper-wrap {
  height: 400rpx;
}

.swiper-wrap .sImage {
  width: 100%;
}

.box {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  justify-content: flex-start;
  /* margin-top: 30rpx; */
}

.box1 {
  background-color: #ffffff;
  width: 335rpx;
  height: 240rpx;
  float: left;
  margin: 20rpx;
  margin-top: 40rpx;
  border-radius: 20rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100rpx 100rpx;
  background-repeat: no-repeat;
  background-position: 100% 100%;
}

