.hjg-authorize-wrap {
  padding: 0 30rpx 46rpx;
  background-color: #fff;
  width: 570rpx;
  border-radius: 12rpx;
}
.hjg-authorize-wrap .weixin {
  width: 120rpx;
  height: 120rpx;
  margin-left: auto;
  margin-right: auto;
  background-image: url(data:image/png;base64,);
}
.hjg-authorize-wrap .text-wrap {
  margin-top: 60rpx;
  color: #333;
  text-align: center;
}
.hjg-authorize-wrap .title {
  padding: 24rpx 0;
  font-size: 36rpx;
  color: #333333;
  text-align: center;
  position: relative;
}
.hjg-authorize-wrap .title:after {
  position: absolute;
  box-sizing: border-box;
  -webkit-transform-origin: center;
  transform-origin: center;
  content: " ";
  pointer-events: none;
  right: 0;
  bottom: 0;
  left: 0;
  border-bottom: 1px solid #E9E9E9;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.hjg-authorize-wrap .info {
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #999999;
  text-align: center;
}
.hjg-authorize-wrap .authbtn-wrap {
  margin-top: 40rpx;
  display: flex;
  justify-content: space-around;
}
.hjg-authorize-wrap .authbtn-wrap button::after {
  appearance: none;
  background: none;
  border: none;
}
.hjg-authorize-wrap .authbtn-wrap .btn {
  width: 240rpx;
  height: 90rpx;
  line-height: 60rpx;
  background: #F2F2F2;
  border-radius: 50rpx;
  font-size: 30rpx;
  color: #09BB07;
  border-color: #F2F2F2;
}
.hjg-authorize-wrap .authbtn-wrap .btn.auth-btn {
  border-color: #09BB07;
  background: #09BB07;
  color: #FFFFFF;
}
